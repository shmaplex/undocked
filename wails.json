{
  "$schema": "https://wails.io/schemas/config.v2.json",
  "name": "Undocked",
  "outputfilename": "undocked",
  "frontend:dir": "frontend",
  "frontend:install": "npm install",
  "frontend:build": "npm run build",
  "frontend:dev:watcher": "npm run dev",
  "frontend:dev:serverUrl": "http://localhost:5173",
  "viteServerTimeout": 10,
  "wailsjsdir": "frontend",
  "debounceMS": 100,
  "author": {
    "name": "Shmaplex",
    "email": "team@shmaplex.com"
  },
  "info": {
    "companyName": "Shmaplex",
    "productName": "Undocked",
    "productVersion": "1.0.0",
    "copyright": "© 2025 Shmaplex",
    "comments": "Run, share, and collaborate on Docker services peer-to-peer—no central servers, just pick a service, host it, and broadcast it to friends.",
    "fileAssociations": [
      {
        "ext": "png",
        "name": "PNG Image",
        "description": "Image File",
        "iconName": "appicon",
        "role": "Editor"
      }
    ]
  },
  "bindings": {
    "ts_generation": {
      "prefix": "",
      "suffix": "",
      "outputType": "classes"
    }
  },
  "obfuscated": false,
  "runNonNativeBuildHooks": false
}